<table class="table">
  <thead>
    <tr>
     
      <th scope="col">Title</th>
      <th scope="col">Price</th>
      <th scope="col">Thumbnail</th>
    </tr>
  </thead>
  <tbody>
      {{#each product}}
    <tr>
    
          <td>{{title}}</td>
          <td>{{price}}</td>
          <td>{{thumbnail}}</td>
    </tr>
     {{/each}}
  </tbody>
</table>

